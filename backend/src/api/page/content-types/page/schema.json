      "components": [
        "sections.hero",
        "sections.bottom-actions",
        "sections.feature-columns-group",
        "sections.feature-rows-group",
        "sections.testimonials-group",
        "sections.large-video",
        "sections.rich-text",
        "sections.pricing",
        "sections.lead-form",
        "sections.features",
        "sections.trending-article",
        "sections.explore-conditions",
        "sections.featured-content",
        "shared.video-embed",
        "elements.logos",
        "elements.feature-row",
        "elements.feature-column",
        "shared.rich-text",
        "sections.heading",
        "elements.plan",
        "sections.contact",
        "medical.medical-disclaimer",
        "medical.emergency-alert",
        "health.health-facts",
        "health.symptom-list",
        "health.treatment-options"
      ]
    },
    "slug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "regex": "^$|^[a-zA-Z/-]+$"
    },
    "heading": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    }
  }
